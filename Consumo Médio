var
dp,tc, cm: real
inicio
escreva("Digite a distância percorrida.:")
leia(dp)
escreva("Digite o total de combustível gasto [lt].:")
leia(tc)
cm <- dp/tc
escreva("O consumo médio do automovél é de ",cm," Km por Lt")
