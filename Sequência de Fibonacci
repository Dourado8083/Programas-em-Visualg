 num1 <- 0
 num2 <- 1
 escreva(num1)
 escreva(num2)
 PARA cont <- 3 ATÉ 8 FAÇA
 res <- num1 + num2
 Escreva(res)
 num1 <- num2
 num2 <- res
fimpara
